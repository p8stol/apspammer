-- Falling snow particles on title
local function createSnowEffect(parent)
    for i = 1, 25 do
        local snow = Instance.new("Frame")
        snow.Size = UDim2.new(0, math.random(2, 4), 0, math.random(2, 4))
        snow.Position = UDim2.new(math.random(0, 100) / 100, 0, -0.1, 0)
        snow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        snow.BorderSizePixel = 0
        snow.BackgroundTransparency = math.random(30, 70) / 100
        snow.ZIndex = 2
        snow.Parent = parent
        
        local snowCorner = Instance.new("UICorner")
        snowCorner.CornerRadius = UDim.new(1, 0)
        snowCorner.Parent = snow
        
        local fallDuration = math.random(3, 6)
        local function animateSnow()
            while snow and snow.Parent do
                local tween = TweenService:Create(
                    snow,
                    TweenInfo.new(fallDuration, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut),
                    {Position = UDim2.new(snow.Position.X.Scale, 0, 1.1, 0)}
                )
                tween:Play()
                tween.Completed:Wait()
                snow.Position = UDim2.new(math.random(0, 100) / 100, 0, -0.1, 0)
            end
        end
        
        task.spawn(animateSnow)
    end
end

createSnowEffect(titleLabel)

